#include "push_swap.h"
#include <stdio.h>

void change_head(t_stack *aa)
{
	aa->head = new_node(1);
}

void f(t_stack *aa)
{
	stack_pop(aa);
}

int	main(void)
{
	t_stack	*aa;

	aa = init_stack();
	printf("head : %p\n", aa->head);
	change_head(aa);
	printf("head : %p\n", aa->head);
	// stack_pop(aa);
	clear_stack(aa);
	// f(aa);
	printf("head : %p\n", aa->head);

	return (0);
}